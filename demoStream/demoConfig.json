{
    "cameraName": "demo",
    "deviceUrl": "rtsp://127.0.0.1:8553/lo",
    "hiResDeviceUrl": "rtsp://127.0.0.1:8554/hi",
    "useEmbeddedSSDMobileNetV1Model": true,
    "modelFile": "models/mobilenetV1/ssd_mobilenet_v1_coco_2017_11_17/frozen_inference_graph.pb",
    "modelConfig": "models/mobilenetV1/ssd_mobilenet_v1_coco_2017_11_17.pbtxt",
    "printDebug": false,
    "video": {
        "hiResPath": "rec/hi",
        "loResPath": "rec/lo",
        "recodeTsToMp4": true
    },
    "motion": {
        "objectMinThreshold": 0.3,
        "lookForClasses": ["car", "truck", "person", "bicycle", "motorcycle", "bus", "cat", "dog", "boat"],
        "embeddedObjectDetector": true,
        "networkObjectDetectServer": "",
        "prebufferSeconds": 5,
        "eventGap": 10
    },
    "pixelMotionAreaThreshold": 50.00,
    "objectCenterMovementThreshold": 50.0,
    "objectAreaThreshold": 2000.0,
    "ignoreAreasClasses": [
        {"class": ["template"], "coordinates": "0,0,0,0"}
    ],
    "streamDrawIgnoredAreas": true,
    "enableOutputStream": true,
    "outputStreamAddr": ":8040"
}